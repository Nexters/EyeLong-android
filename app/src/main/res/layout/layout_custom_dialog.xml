<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root_custom_dialog"
    android:layout_width="248dp"
    android:layout_height="168dp"
    android:background="@drawable/bg_custom_dialog_white"
    android:elevation="2dp">

    <TextView
        android:id="@+id/tv_custom_dialog_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/notosanscjkkrbold"
        android:includeFontPadding="false"
        android:textColor="@color/colorBlack_900"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_custom_dialog_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/notosanscjkkrmedium"
        android:includeFontPadding="false"
        android:textColor="@color/colorBlack_500"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_custom_dialog_title"
        app:layout_constraintStart_toStartOf="@+id/tv_custom_dialog_title"
        app:layout_constraintTop_toBottomOf="@+id/tv_custom_dialog_title" />

    <Button
        android:id="@+id/btn_custom_dialog_cancel"
        android:layout_width="86dp"
        android:layout_height="34dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/bg_btn_round_box_white"
        android:fontFamily="@font/notosanscjkkrbold"
        android:includeFontPadding="false"
        android:textColor="@color/colorBlack_500"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn_custom_dialog_confirm"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_custom_dialog_confirm"
        android:layout_width="86dp"
        android:layout_height="34dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/bg_btn_round_box_green"
        android:fontFamily="@font/notosanscjkkrbold"
        android:includeFontPadding="false"
        android:textColor="@color/colorBlack_500"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btn_custom_dialog_cancel" />
</androidx.constraintlayout.widget.ConstraintLayout>