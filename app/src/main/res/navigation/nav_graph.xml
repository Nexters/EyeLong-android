<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/eyeExerciseGuideFragment">

    <fragment
        android:id="@+id/eyeExerciseGuideFragment"
        android:name="com.teamnexters.eyelong.ui.exercise.fragment.EyeExerciseGuideFragment"
        android:label="fragment_eye_exercise_guide"
        tools:layout="@layout/fragment_eye_exercise_guide">
        <action
            android:id="@+id/action_eyeExerciseGuideFragment_to_eyeExerciseCountdownFragment"
            app:destination="@id/eyeExerciseCountdownFragment" />
    </fragment>
    <fragment
        android:id="@+id/eyeExerciseCountdownFragment"
        android:name="com.teamnexters.eyelong.ui.exercise.fragment.EyeExerciseCountdownFragment"
        android:label="fragment_eye_exercise_countdown"
        tools:layout="@layout/fragment_eye_exercise_countdown">
        <action
            android:id="@+id/action_eyeExerciseCountdownFragment_to_eyeExerciseLottieFragment"
            app:destination="@id/eyeExerciseLottieFragment" />
        <argument
            android:name="dataArg"
            app:argType="com.teamnexters.eyelong.db.entity.Exercise" />
    </fragment>
    <fragment
        android:id="@+id/eyeExerciseLottieFragment"
        android:name="com.teamnexters.eyelong.ui.exercise.fragment.EyeExerciseLottieFragment"
        android:label="fragment_eye_exercise_lottie"
        tools:layout="@layout/fragment_eye_exercise_lottie">
        <argument
            android:name="dataArg"
            app:argType="com.teamnexters.eyelong.db.entity.Exercise" />
        <action
            android:id="@+id/action_eyeExerciseLottieFragment_to_eyeExerciseFinishFragment"
            app:destination="@id/eyeExerciseFinishFragment" />
    </fragment>
    <fragment
        android:id="@+id/eyeExerciseFinishFragment"
        android:name="com.teamnexters.eyelong.ui.exercise.fragment.EyeExerciseFinishFragment"
        android:label="fragment_eye_exercise_finish"
        tools:layout="@layout/fragment_eye_exercise_finish">
        <action
            android:id="@+id/action_eyeExerciseFinishFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.teamnexters.eyelong.ui.main.activity.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>